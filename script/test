#!/bin/bash -exu

cd `dirname $0`/..

stack build --stack-yaml=./stack.ghcjs.yaml --flag rfc:development 
stack build --flag rfc:development

stack clean 
stack clean --stack-yaml=./stack.ghcjs.yaml 
stack build --stack-yaml=./stack.ghcjs.yaml --flag rfc:development 

stack clean 
stack clean --stack-yaml=./stack.ghcjs.yaml 
stack build --flag rfc:development
